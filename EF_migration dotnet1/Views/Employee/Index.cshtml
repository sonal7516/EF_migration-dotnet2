@model IEnumerable<EF_migration_dotnet1.Model.Employee>

<h1 style="color:crimson; font-display:block; font-family:'Global Sans Serif'">Employee Details</h1>

<table class="table table-bordered">
    <tr style="background-color:aquamarine">
        <th>ID</th>
        <th>Name</th>
        <th>Email_ID</th>
        <th>Gender</th>
        <th>Mobile</th>
        <th>City</th>
    </tr>'
    
    @foreach (var emp in Model)
    {
<tr style="background-color:aqua">
    <td>@emp.ID</td>
    <td>@emp.Name</td>
    <td>@emp.Email</td>
    <td>@emp.Gender</td>
    <td>@emp.Mobile</td>
    <td>@emp.City</td>
</tr>
    }
</table>